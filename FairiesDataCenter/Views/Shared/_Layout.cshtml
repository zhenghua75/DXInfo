@using Trirand.Web.Mvc
@using ynhnTransportManage
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@Html.DataCenterTitle()@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/redmond/jquery-ui-1.8.21.custom.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/ui.jqgrid.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/menu/superfish.css")" media="screen"/>
    <link href="@Url.Content("~/Content/themes/ui.multiselect.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-2.0.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.datepicker-zh-CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery-Timepicker-Addon/jquery-ui-timepicker-addon.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery-Timepicker-Addon/localization/jquery-ui-timepicker-zh-CN.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jQuery-Timepicker-Addon/jquery-ui-timepicker-addon.css")"
        rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jQuery-Timepicker-Addon/jquery-ui-sliderAccess.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/trirand/ui.multiselect.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/trirand/i18n/ui-multiselect-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/trirand/i18n/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/trirand/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/menu/superfish.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/menu/hoverIntent.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MyJs.js")" type="text/javascript"></script>    
    <script src="@Url.Content("~/Scripts/MyJs2.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/jquery.contextmenu.r2.js")" type="text/javascript"></script>  
</head>
<body>
<div id="IsHaveCardNoUrl" data-url="@Url.Action("IsHaveCardNo", "Card")"></div>
<div id="LogOffUrl" data-url="@Url.Action("LogOff", "Account")"></div>
<div id="IsAMSApp" data-url="@Html.IsAMSApp()"></div>
<div id="IsNoActiveXCheck" data-url="@Html.IsNoActiveXCheck()"></div>
@if (!System.Configuration.ConfigurationManager.AppSettings.AllKeys.Contains("AMSApp"))
{
    <OBJECT ID="kk" WIDTH=0 HEIGHT=0 style="display:none"
	 CLASSID="CLSID:01A8EC23-6702-476C-AF5F-E934C71D0FE6" codebase="@Url.Content("~/Content/DXInfoCardActiveX.CAB")#version=1,0,0,3">
    </object>
}
    <div class="wrapper">
        <div id="logo">
            <h1>
                 <a href="/">@Html.DataCenterTitle()</a>
            </h1>
            <p>
                <em>@Html.Partial("_LogOnPartial")</em></p>
        </div>
        <hr />
        <div id="header">
            <div id="menu">
                <ul class="sf-menu">
                    @Html.Menu()
                </ul>
            </div>
        </div>
        <div id="page">
            <div id="page-bgtop">
                <div id="content">
                    @RenderBody()
                    @Html.Trirand().JQDatePicker(
            new JQDatePicker
            {
                DisplayMode = DatePickerDisplayMode.ControlEditor,
                ShowOn = ShowOn.Focus,
                DateFormat = "yyyy-mm-dd",
            }, "DatePicker")
                    @Html.Trirand().JQDateTimePicker(
            new JQDateTimePicker
            {
            }, "DateTimePicker")
            @Html.Trirand().JQTimePicker(
                         new JQTimePicker(), "TimePicker"
            )
                </div>
                <div class="contextMenu" id="jqGridContextMenu" style="width:250px;display:none">
        <ul style="width: 250px">
            <li id="add" >
            <div class="ui-state-default">
                <span class="ui-icon ui-icon-plus" style="float:left"></span>
                <span >添加</span>
                </div>
            </li>
            <li id="edit">
            <div class="ui-state-default">
                <span class="ui-icon ui-icon-pencil" style="float:left"></span>
                <span>编辑</span>
                </div>
            </li>   
            <li id="view">
            <div class="ui-state-default">
                <span class="ui-icon ui-icon-document" style="float:left"></span>
                <span>查看</span>
                </div>
            </li>               
            <li id="del">
            <div class="ui-state-default">
                <span class="ui-icon ui-icon-trash" style="float:left"></span>
                <span>删除</span>
                </div>
            </li>   
            <li id="search">
            <div class="ui-state-default">
                <span class="ui-icon ui-icon-search" style="float:left"></span>
                <span>查找</span>
                </div>
            </li>              
        </ul>
    </div>
                <div style="clear: both;">
                    &nbsp;</div>
            </div>
        </div>
        <div id="footer-bgcontent">
            <div id="footer">
                <p>
                    Copyright ©2011 - 2013 DXInfo. All Rights Reserved. Design by <a href="http://www.kmdx.cn">
                        昆明道讯科技有限公司</a>.</p>
            </div>
        </div>
    </div>
</body>
</html>
