                    Grid.ColumnSpan="2"
                    Grid.Column="0"
                    IsHitTestVisible="False"
                    Opacity="0"
                    Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Grid.Column="0"
                           Fill="#A5FFFFFF"
                           RadiusY="1"
                           Grid.Row="0"
                           RadiusX="1" />
                                <Rectangle Grid.Column="1"
                           Fill="#A5FFFFFF"
                           Height="18"
                           Margin="3,0,3,0"
