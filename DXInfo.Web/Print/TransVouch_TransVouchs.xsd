                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT   TransVouch.Id, TransVouch.Code, TransVouch.OutRdCode,a.Name OutRdName, TransVouch.InRdCode,b.Name InRdName, TransVouch.TVDate, 
                TransVouch.OutWhId,c.Name OutWhName, TransVouch.InWhId,d.Name InWhName, TransVouch.OutDeptId,e.DeptName OutDeptName,TransVouch.InDeptId,p.DeptName InDeptName, TransVouch.Memo, TransVouch.Maker,
                f.FullName MakerName, 
                TransVouch.MakeDate, TransVouch.MakeTime, TransVouch.IsVerify, TransVouch.Verifier,g.FullName VerifierName, TransVouch.VerifyTime, 
                TransVouch.VerifyDate, TransVouch.Modifier,h.FullName ModifierName, TransVouch.ModifyDate, TransVouch.ModifyTime, TransVouch.Salesman, i.FullName SalesmanName,
                TransVouchs.Id as SubId, TransVouchs.InvId,j.Name InvName,j.Specs, TransVouchs.MainUnit,k.Name MainUnitName, TransVouchs.STUnit,l.Name STUnitName, TransVouchs.ExchRate, 
                TransVouchs.Quantity, TransVouchs.Num, TransVouchs.Price, TransVouchs.Amount, TransVouchs.Batch, 
                TransVouchs.MadeDate, TransVouchs.ShelfLife, TransVouchs.ShelfLifeType,m.Description ShelfLifeTypeName, TransVouchs.InvalidDate, 
                TransVouchs.Locator,n.Name LocatorName
FROM      TransVouch 
INNER JOIN TransVouchs ON TransVouch.Id = TransVouchs.TVId
left join RdType a on TransVouch.OutRdCode=a.Code
left join RdType b on TransVouch.InRdCode=b.Code
left join Warehouse c on TransVouch.OutWhId=c.Id
left join Warehouse d on TransVouch.InWhId=d.Id
left join Depts e on TransVouch.OutDeptId=e.DeptId
left join Depts p on TransVouch.InDeptId=p.DeptId
left join aspnet_CustomProfile f on TransVouch.Maker=f.UserId
left join aspnet_CustomProfile g on TransVouch.Verifier=g.UserId
left join aspnet_CustomProfile h on TransVouch.Modifier=h.UserId
left join aspnet_CustomProfile i on TransVouch.Salesman=i.UserId
left join Inventory j on TransVouchs.InvId=j.Id
left join UnitOfMeasures k on TransVouchs.MainUnit=k.Id
left join UnitOfMeasures l on TransVouchs.STUnit=l.Id
left join (select * from EnumTypeDescription where Code='ShelfLifeType') m on TransVouchs.ShelfLifeType=m.Value
left join Locator n on TransVouchs.Locator=n.Id
where TransVouch.Id=@Id</CommandText>
